<%- include('partials/top'); %>
<!-- page content -->
<div class="right_col" role="main">
  <div class="main" role="main">
    <div id="content" class="content full">
    <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>Service Records</h2>
        <div class="clearfix"></div>
      </div>
      <div class="x_content"> 
        <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
          <thead>
            <tr>
              <th>Customer</th>
              <th>Service</th>
              <th>Vehicle</th>
              <th>License Plate</th>
              <th>Parts Required</th>
              <th>Parts Available</th>
              <th>Time Open</th>
              <th>Time In</th>
              <th>Time Out</th>
              <th>Status</th>
            </tr>
          </thead>
          <tbody>
          <% for(var i=0; i<10; i++){ %>
            <tr>
              <td>Mike Edward Brown</td>
              <td>Transmission Failure</td>
              <td>2011 Honda Accord - White</td>
              <td>BUY0M5</td>
              <td>Break Clutch, Washer fluid tank</td>
              <td>Washer fluid tank</td>
              <td>Nov. 18, 9:52 PM</td>
              <td>Nov. 19, 10:45 AM</td>
              <td>Nov. 19, 10:45 AM</td>
              <%if(false){ %>
              <td>CLOSED</td>
              <%}else{%>
                <td>
                    <!-- Split button -->
                    <div class="btn-group">
                        <button data-toggle="dropdown" class="btn btn-primary dropdown-toggle btn-sm" type="button" aria-expanded="false">Start <span class="caret"></span>
                        </button>
                        <ul role="menu" class="dropdown-menu">
                        <li><a href="#">Start</a>
                        </li>
                        <li><a href="#">Add Notes</a>
                        </li>
                        <li class="divider"></li>
                        <li><a href="#">Delete</a>
                        </li>
                        </ul>
                        </div>
                </td>
              <%}%>
            </tr>
          <%}%>
          </tbody>
        </table>
        
        
      </div>
    </div>
  </div>
</div>
</div>
</div>
<%- include('partials/bottom'); %>